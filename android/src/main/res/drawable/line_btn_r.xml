<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item
        android:bottom="-2dp"
        android:left="1dp"
        android:right="-2dp"
        android:top="-2dp">
        <shape android:shape="rectangle" >
            <stroke
                android:width="1dp"
                android:color="#FFFFFF" />

            <solid android:color="#00000000" />

            <corners
                android:topRightRadius="25dp"
                />
        </shape>
    </item>

</layer-list>

